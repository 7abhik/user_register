<style>
	.container {
		display: flex;
		justify-content: center;
	}
</style>


<div class="container">
	<table class="table">
		<thead>
			<tr>
				<th scope="col">#</th>
				<th scope="col">Name</th>
				<th scope="col">Email</th>
			</tr>
		</thead>
		<tbody>
			<% users.forEach(user=> {%>
				<tr>
					<th scope="row"><%= user.uId%></th>
					<td><%= user.name%></td>
					<td><%= user.email%></td>
				</tr>
				<%})%>

		</tbody>
	</table>
</div>